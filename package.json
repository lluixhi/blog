{
    "name": "bloggo",
    "version": "1.0.0",
    "description": "simple blog app using typescript and react",
    "main": "index.jsx",
    "scripts": {
        "serve": "npm-run-all --parallel serve:build-serve serve:build-dev",
        "start": "nodemon build/server.js",
        "serve:build-serve": "webpack --mode development --config webpack.server.js --watch",
        "serve:build-dev": "webpack --config webpack.dev.js --watch",
        "build": "webpack --mode production --config webpack.server.js && webpack --config webpack.prod.js"
    },
    "author": "Ariadne Belsito",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.1",
        "pg": "^8.4.1",
        "pg-native": "^3.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "reflect-metadata": "^0.1.13",
        "regenerator-runtime": "^0.13.7",
        "typeorm": "^0.2.28"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-decorators": "^7.10.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/express": "^4.17.8",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.8",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.1",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.3.0",
        "html-loader": "^1.3.0",
        "html-webpack-plugin": "^4.4.1",
        "jest": "^26.4.2",
        "mini-css-extract-plugin": "^0.11.2",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.5",
        "npm-run-all": "^4.1.5",
        "pnp-webpack-plugin": "^1.6.4",
        "sass-loader": "^10.0.2",
        "style-loader": "^1.2.1",
        "typescript": "^4.0.2",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^5.1.4",
        "webpack-node-externals": "^2.5.2"
    }
}
